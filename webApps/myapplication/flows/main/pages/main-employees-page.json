{
  "title": "main employees",
  "description": "",
  "variables": {
    "employeeListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Employee",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "getallEmployeeResponse2"
      }
    },
    "employeeListSDP2": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Employee",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "getallEmployeeResponse3"
      }
    },
    "oj_table_372858639_1SelectedId": {
      "type": "string",
      "defaultValue": ""
    }
  },
  "metadata": {},
  "types": {
    "@decorators.getallEmployeeResponse": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "@decorators.getallEmployeeResponse2": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "@decorators.getallEmployeeResponse3": {
      "endpoint": "getall_Employee",
      "service": "businessObjects",
      "path": "response"
    },
    "getallEmployeeResponse": {
      "items": [
        {
          "departmentObject": {
            "items": [
              {
                "department": "string"
              }
            ]
          },
          "id": "number",
          "name": "string",
          "picture": "string"
        }
      ]
    },
    "getallEmployeeResponse2": {
      "items": [
        {
          "departmentObject": {
            "items": [
              {
                "department": "string"
              }
            ]
          },
          "id": "number",
          "name": "string",
          "picture": "string"
        }
      ]
    },
    "getallEmployeeResponse3": {
      "items": [
        {
          "id": "number",
          "name": "string",
          "salary": "number"
        }
      ]
    }
  },
  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chainId": "ButtonActionChain"
        }
      ]
    },
    "createEmployeeButtonClicked": {
      "chains": [
        {
          "chainId": "navigateToCreateEmployeeChain"
        }
      ]
    },
    "deleteEmployeeButtonClicked": {
      "chains": [
        {
          "chainId": "deleteEmployeeChain",
          "parameters": {
            "employeeId": "[[ $variables.oj_table_372858639_1SelectedId ]]"
          }
        }
      ]
    },
    "editEmployeeButtonClicked": {
      "chains": [
        {
          "chainId": "navigateToEditEmployeeChain",
          "parameters": {
            "employeeId": "[[ $variables.oj_table_372858639_1SelectedId ]]"
          }
        }
      ]
    },
    "oj_table_372858639_1SelectionChanged": {
      "chains": [
        {
          "chainId": "oj_table_372858639_1ChangeSelectionChain",
          "parameters": {
            "employeeId": "[[ $event.detail.value.key || '' ]]"
          }
        }
      ]
    },
    "toggleDrawer": {
      "chains": [
        {
          "chainId": "application:toggleDrawerHandler"
        }
      ]
    }

  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-chart": {
        "path": "ojs/ojchart"
      },
      "oj-chart-item": {
        "path": "ojs/ojchart"
      },
      "oj-table": {
        "path": "ojs/ojtable"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      }
    }
  }
}