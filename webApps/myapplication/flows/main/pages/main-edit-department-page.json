{
  "title": "Edit Department",
  "description": "Updates single Department record",
  "variables": {
    "department": {
      "type": "page:getDepartmentResponse"
    },
    "departmentETag": {
      "type": "string"
    },
    "departmentId": {
      "type": "string",
      "required": true,
      "input": "fromUrl"
    },
    "locationListSDP": {
      "type": "vb/ServiceDataProvider",
      "defaultValue": {
        "endpoint": "businessObjects/getall_Location",
        "keyAttributes": "id",
        "itemsPath": "items",
        "responseType": "getallLocationResponse",
        "uriParameters": {}
      }
    },
    "saveDepartmentChainInProgress": {
      "type": "boolean",
      "defaultValue": false
    }
  },
  "metadata": {},
  "types": {
    "@decorators.getallLocationResponse": {
      "endpoint": "getall_Location",
      "service": "businessObjects",
      "path": "response"
    },
    "@decorators.getDepartmentResponse": {
      "endpoint": "get_Department",
      "service": "businessObjects",
      "path": "response"
    },
    "businessObjectsServiceErrorResponseType": {
      "body": {
        "status": "string",
        "title": "string",
        "o:errorDetails": [
          {
            "detail": "string",
            "o:errorCode": "string"
          }
        ]
      },
      "status": "number"
    },
    "getallLocationResponse": {
      "items": [
        {
          "id": "number",
          "location": "string"
        }
      ]
    },
    "getDepartmentResponse": {
      "department": "string",
      "location": "number"
    }
  },
  "eventListeners": {
    "backButtonClicked": {
      "chains": [
        {
          "chainId": "goBackChain"
        }
      ]
    },
    "saveButtonClicked": {
      "chains": [
        {
          "chainId": "saveDepartmentChain"
        }
      ]
    },
    "toggleDrawer": {
      "chains": [
        {
          "chainId": "application:toggleDrawerHandler"
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "chainId": "loadDepartmentChain"
        }
      ]
    }
  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-select-single": {
        "path": "ojs/ojselectsingle"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      },
      "oj-validation-group": {
        "path": "ojs/ojvalidationgroup"
      }
    }
  }
}